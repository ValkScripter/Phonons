{
    "version": 8,
    "configurePresets": [
        {
            "name": "GCC_11.4_x86_64-linux",
            "displayName": "GCC 11.4.0 x86_64-linux-gnu",
            "description": "Using compilers: C = /usr/bin/gcc, CXX = /usr/bin/g++",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/install",
                "CMAKE_C_COMPILER": "/usr/bin/gcc",
                "CMAKE_CXX_COMPILER": "/usr/bin/g++",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "GCC_14.2_aarch64-apple",
            "displayName": "GCC 14.2.0 aarch64-apple-darwin23",
            "description": "Utilisation des compilateursÂ : C = /opt/homebrew/bin/aarch64-apple-darwin23-gcc-14, CXX = /opt/homebrew/bin/aarch64-apple-darwin23-g++-14",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/install",
                "CMAKE_C_COMPILER": "/opt/homebrew/bin/aarch64-apple-darwin23-gcc-14",
                "CMAKE_CXX_COMPILER": "/opt/homebrew/bin/aarch64-apple-darwin23-g++-14",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "GCC_13.2_aarch64-linux",
            "displayName": "GCC 13.2.0 aarch64-linux-gnu",
            "description": "Using compilers: C = /usr/bin/aarch64-linux-gnu-gcc, CXX = /usr/bin/aarch64-linux-gnu-g++",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/install",
                "CMAKE_C_COMPILER": "/usr/bin/aarch64-linux-gnu-gcc",
                "CMAKE_CXX_COMPILER": "/usr/bin/aarch64-linux-gnu-g++",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}
